import thread
import time
from threading import *
class itemQ:
    def __init__(self):
        self.count = 0
    def produce(self,num=1):
        self.count+=num
    def consume(self):
        if self.count:self.count-=1
    def isEmpty(self):
        return not self.count

class Producer(Thread):
    
    def __init__(self,condition,itemq,sleeptime=1):
        Thread.__init__(self)
        self.cond = condition    